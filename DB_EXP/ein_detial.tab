create table EIN_DETIAL
(
  id         VARCHAR2(32) not null,
  ein_id     VARCHAR2(32) not null,
  goodsname  VARCHAR2(90),
  num        VARCHAR2(16),
  price      VARCHAR2(16),
  hsbz       VARCHAR2(1),
  taxrate    VARCHAR2(10),
  spec       VARCHAR2(40),
  unit       VARCHAR2(22),
  spbm       VARCHAR2(19),
  zsbm       VARCHAR2(20),
  fphxz      VARCHAR2(1),
  yhzcbs     VARCHAR2(1),
  zzstsgl    VARCHAR2(50),
  lslbs      VARCHAR2(1),
  kce        NUMBER(16,2),
  taxfreeamt NUMBER(16,2),
  tax        NUMBER(16,2),
  taxamt     NUMBER(16,2)
)
;
comment on table EIN_DETIAL
  is '电子发票明细表';
comment on column EIN_DETIAL.id
  is '主键ID';
comment on column EIN_DETIAL.ein_id
  is '电子发票信息表';
comment on column EIN_DETIAL.goodsname
  is '商品名称';
comment on column EIN_DETIAL.num
  is '数量';
comment on column EIN_DETIAL.price
  is '单价';
comment on column EIN_DETIAL.hsbz
  is '单价含税标志，0:不含税,1:含税';
comment on column EIN_DETIAL.taxrate
  is '税率';
comment on column EIN_DETIAL.spec
  is '规格型号';
comment on column EIN_DETIAL.unit
  is '单位';
comment on column EIN_DETIAL.spbm
  is '税收分类编码';
comment on column EIN_DETIAL.zsbm
  is '自行编码';
comment on column EIN_DETIAL.fphxz
  is '发票行性质:0,正常行;1,折扣行;2,被折扣行';
comment on column EIN_DETIAL.yhzcbs
  is '优惠政策标识:0,不使用;1,使用';
comment on column EIN_DETIAL.zzstsgl
  is '增值税特殊管理，如：即征即退、免税、简易征收 等';
comment on column EIN_DETIAL.lslbs
  is '零税率标识:空,非零税率;1,免税;2,不征税;3,普通零税率';
comment on column EIN_DETIAL.kce
  is '扣除额，小数点后两位。差额征收的发票目前只支持一行明细。不含税差额 = 不含税金额 - 扣除额； 税额 = 不含税差额*税率。';
comment on column EIN_DETIAL.taxfreeamt
  is '不含税金额';
comment on column EIN_DETIAL.tax
  is '税额';
comment on column EIN_DETIAL.taxamt
  is '含税金额';

