comment on column YS_YHDWWH.dwmemo
  is '备注';
comment on column YS_YHDWWH.dwonnectper
  is '联系人';
comment on column YS_YHDWWH.dwconnecttel
  is '联系电话';
alter table YS_YHDWWH
  add constraint AK_KEY_DWMID_YS_YHDWWH unique (DWID);

