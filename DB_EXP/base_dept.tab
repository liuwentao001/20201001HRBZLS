create table BASE_DEPT
(
  id               VARCHAR2(32),
  dept_no          VARCHAR2(32),
  dept_name        VARCHAR2(100),
  parent_id        VARCHAR2(32),
  contact_telphone VARCHAR2(60),
  remark           VARCHAR2(1000),
  hire_code        VARCHAR2(32),
  create_date      DATE,
  create_per       VARCHAR2(32),
  is_root          VARCHAR2(2)
)
;

