create table 远传表接收
(
  yhh  VARCHAR2(10) not null,
  cbrq DATE not null,
  bqhd NUMBER not null,
  bk   VARCHAR2(20) not null,
  c1   VARCHAR2(50),
  c2   VARCHAR2(50),
  c3   VARCHAR2(50),
  c4   VARCHAR2(50),
  c5   VARCHAR2(50)
)
;
comment on column 远传表接收.yhh
  is '客户代码';
comment on column 远传表接收.cbrq
  is '抄表日期';
comment on column 远传表接收.bqhd
  is '本期行度';
comment on column 远传表接收.bk
  is '表况';
alter table 远传表接收
  add constraint PK_MICODEYQ primary key (YHH);

