create table OA_MEETING
(
  id               VARCHAR2(32),
  meeting_name     VARCHAR2(255),
  start_date       DATE,
  end_date         DATE,
  purpose          CLOB,
  org_man          VARCHAR2(255),
  org_dept         VARCHAR2(255),
  initiator        VARCHAR2(255),
  initiator_date   DATE,
  addr             VARCHAR2(255),
  status           VARCHAR2(2),
  is_inform_affirm CHAR(1),
  is_inform_cancel CHAR(1),
  hire_code        VARCHAR2(32)
)
;

