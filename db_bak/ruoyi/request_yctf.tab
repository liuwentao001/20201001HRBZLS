comment on column REQUEST_YCTF.modifydate
  is '修改时间';
comment on column REQUEST_YCTF.modifyuserid
  is '修改人ID';
comment on column REQUEST_YCTF.modifyusername
  is '修改人';
comment on column REQUEST_YCTF.remark
  is '备注';
comment on column REQUEST_YCTF.workno
  is '单据编号';
comment on column REQUEST_YCTF.workbatch
