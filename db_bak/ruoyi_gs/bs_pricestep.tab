create table BS_PRICESTEP
(
  pspscid NUMBER default 0 not null,
  pspfid  VARCHAR2(10) not null,
  pspiid  CHAR(2) not null,
  psclass NUMBER not null,
  psscode NUMBER(10) not null,
  psecode NUMBER(10) not null,
  psprice NUMBER(10,3) not null,
  psmemo  VARCHAR2(60)
)
;
comment on table BS_PRICESTEP
  is '阶梯水价明细【PS】';
comment on column BS_PRICESTEP.pspscid
  is '方案编码';
comment on column BS_PRICESTEP.pspfid
  is '费率编码';
comment on column BS_PRICESTEP.pspiid
  is '费率项目代码';
comment on column BS_PRICESTEP.psclass
  is '阶梯级别';
comment on column BS_PRICESTEP.psscode
  is '起量';
comment on column BS_PRICESTEP.psecode
  is '止量';
comment on column BS_PRICESTEP.psprice
  is '单价';
comment on column BS_PRICESTEP.psmemo
  is '备注';

