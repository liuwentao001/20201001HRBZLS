create table HRBZLS.EZZLS_OLDREC
(
  eono       VARCHAR2(10) not null,
  eoname     VARCHAR2(50),
  eocode     VARCHAR2(200),
  eomoneysf  NUMBER(13,3),
  eomoneyfj  NUMBER(13,3),
  eomoneysz  NUMBER(13,3),
  eomoneyps  NUMBER(13,3),
  eomoneypw  NUMBER(13,3),
  eomoneyzw  NUMBER(13,3),
  eomoneyqt  NUMBER(13,3),
  eomoneyhj  NUMBER(13,3),
  eomemo1    VARCHAR2(300),
  eomemo2    VARCHAR2(50),
  eopaidflag CHAR(1),
  eopaidtime DATE,
  eopaidper  VARCHAR2(50)
)
tablespace USERS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
comment on column HRBZLS.EZZLS_OLDREC.eono
  is '编号';
comment on column HRBZLS.EZZLS_OLDREC.eoname
  is '户名';
comment on column HRBZLS.EZZLS_OLDREC.eocode
  is '户号';
comment on column HRBZLS.EZZLS_OLDREC.eomoneysf
