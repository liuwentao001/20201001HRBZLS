CREATE OR REPLACE FUNCTION HRBZLS."FSYSMANALBBM"(TCODE IN CHAR, LLB IN CHAR)
  RETURN CHAR AS
  LCODE VARCHAR2(15);
BEGIN
  SELECT SMFID
    INTO LCODE
    FROM SYSMANAFRAME
   WHERE SMFTYPE = LLB
   START WITH SMFID = TCODE
  CONNECT BY PRIOR SMFPID = SMFID;
  RETURN LCODE;
EXCEPTION
  WHEN OTHERS THEN
    --LCODE := fsysmanalbbm(TCODE,'2');--20140515 非地区用户返回部门
    LCODE := NULL;
    RETURN LCODE;
END FSYSMANALBBM;
/

