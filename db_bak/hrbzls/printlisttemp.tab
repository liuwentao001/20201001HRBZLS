create global temporary table HRBZLS.PRINTLISTTEMP
(
  pltno      NUMBER(10),
  pltcontent VARCHAR2(4000)
)
on commit delete rows;
comment on table HRBZLS.PRINTLISTTEMP
  is '打印临时表[PLT]';
comment on column HRBZLS.PRINTLISTTEMP.pltno
  is '序号';
comment on column HRBZLS.PRINTLISTTEMP.pltcontent
  is '打印内容';

